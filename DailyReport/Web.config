  <connectionStrings>
    <add name="myConnectionString" connectionString="Data Source=SWAPYY\SQLEXPRESS;Initial Catalog=mybusiness;Integrated Security=true"/>
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
